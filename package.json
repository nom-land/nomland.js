{
  "name": "nomland.js",
  "version": "1.0.0",
  "description": "nomland.js is a library to interact with the nomland protocol.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsup --dts",
    "prepublish": "npm run build"
  },
  "keywords": [
    "nomland"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/md5": "^2.3.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@extractus/article-extractor": "^7.3.1",
    "browser-bunyan": "^1.8.0",
    "bunyan": "^1.8.15",
    "crossbell": "^1.6.1",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "eip1193-types": "^0.2.1",
    "md5": "^2.3.0",
    "punycode": "^2.3.0",
    "viem": "^1.6.7"
  }
}
