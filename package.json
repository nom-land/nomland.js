{
  "name": "nomland.js",
  "version": "0.2.2",
  "description": "nomland.js is a library to interact with the nomland protocol.",
  "main": "./dist/node/index.node.js",
  "types": "./dist/node/index.node.d.ts",
  "module": "./dist/node/index.node.mjs",
  "browser": "dist/browser/index.browser.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "url": "https://github.com/nom-land/nomland.js.git",
  "exports": {
    "./node": {
      "require": "./dist/node/index.node.js",
      "import": "./dist/node/index.node.mjs",
      "types": "./dist/node/index.node.d.ts"
    },
    "./browser": {
      "require": "./dist/browser/index.browser.js",
      "import": "./dist/browser/index.browser.js",
      "types": "./dist/browser/index.d.mts"
    },
    ".": "./dist/node/index.node.js"
  },
  "scripts": {
    "dev": "nodemon src/run.ts",
    "build": "tsup --dts",
    "lint": "tsc --noEmit",
    "prepublish": "npm run build",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "nomland"
  ],
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/bunyan": "^1.8.8",
    "@types/md5": "^2.3.2",
    "@types/node": "20.5.4",
    "@types/punycode": "^2.1.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@extractus/article-extractor": "^7.3.1",
    "@nomland/elephant": "^0.2.4",
    "@urql/core": "^4.1.3",
    "browser-bunyan": "^1.8.0",
    "bunyan": "^1.8.15",
    "crossbell": "^1.6.4",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "eip1193-types": "^0.2.1",
    "md5": "^2.3.0",
    "punycode": "^2.3.0",
    "viem": "^1.10.8"
  }
}
